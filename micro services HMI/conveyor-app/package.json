    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.1",
    "vue-cli-plugin-single-spa": "^1.1.0",
    "vue-template-compiler": "^2.6.11"
