{
  "name": "conveyor-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve --port 8081",
    "sorting": "cd ~/work/SDMHmi/HMI/sorting-app && npm run serve",
