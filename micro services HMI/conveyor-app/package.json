  "dependencies": {
    "all": "0.0.0",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.6.1",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "single-spa-vue": "^1.8.1",
    "systemjs-webpack-interop": "^1.1.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
